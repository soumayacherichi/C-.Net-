    <hr>
    <table class="table table-striped">
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Is Friendly</th>
            <th>Color</th>
        </tr>
        @{
            foreach (Pet p in Model)
            {
                <tr>
                    <td>@p.Name</td>
                    <td>@p.Age</td>
